<%# encoding: utf-8 %>

$(document).ready(function() {

              var skills = [];
		      <% for skill in Skill.all %>
		           skills.push('<%= skill.name %>');
		      <% end %>	              

    	     $("#e2").select2({ tags:skills,
                                tokenSeparators: [","],
                                placeholder: "Select or type new skill ..."});

});

